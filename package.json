{
  "name": "itunes-label-applicator",
  "version": "0.1.0",
  "description": "Adds the record label's name to Songs' Grouping fields and creates a Labels: <label-name> Smart Playlist",
  "main": "index.js",
  "scripts": {
    "commitmsg": "commitplease",
    "precommit": "npm run commitmsg",
    "pretest": "eslint . --fix",
    "test": "jest --config=jest.config.json",
    "prepublish": "nsp check",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gregswindle/itunes-label-applicator.git"
  },
  "keywords": [
    "itunes",
    "grouping",
    "record-label",
    "smart-playlist"
  ],
  "author": "Greg Swindle <greg@swindle.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gregswindle/itunes-label-applicator/issues"
  },
  "homepage": "https://github.com/gregswindle/itunes-label-applicator#readme",
  "devDependencies": {
    "bithound": "^1.7.0",
    "commitplease": "^3.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.12.1",
    "eslint-config-xo-space": "^0.21.0",
    "git-labelmaker": "^0.9.0",
    "jest": "^24.0.0",
    "jest-cli": "^24.0.0",
    "js-yaml": "^3.8.3",
    "nsp": "^3.2.1",
    "semantic-release": "^15.13.3",
    "semantic-release-cli": "^4.1.0"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "jest": true,
      "node": true
    }
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "build",
      "ci",
      "chore",
      "revert"
    ],
    "scope": "\\S+.*"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "denodeify": "^1.2.1",
    "lodash": "^4.17.4",
    "xml2js": "^0.4.17"
  }
}
